<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Dates_Rule</fullName>
    <active>true</active>
    <errorConditionFormula>(Start_Date__c &gt;=  End_Date__c)||( Start_Date__c &lt; TODAY() )</errorConditionFormula>
    <errorDisplayField>Start_Date__c</errorDisplayField>
    <errorMessage>Invalid dates</errorMessage>
</ValidationRule>
