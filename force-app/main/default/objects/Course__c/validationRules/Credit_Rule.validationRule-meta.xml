<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Credit_Rule</fullName>
    <active>true</active>
    <errorConditionFormula>( Credit__c &gt;100)|| ( Credit__c &lt;1)</errorConditionFormula>
    <errorDisplayField>Credit__c</errorDisplayField>
    <errorMessage>Credit must be between 1 and 100</errorMessage>
</ValidationRule>
